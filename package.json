{
  "name": "jschess",
  "version": "1.0.0",
  "description": "Multiplayer real time chess",
  "keywords": [
    "chess",
    "javascript",
    "games",
    "real time"
  ],
  "scripts": {
    "dev": "parallelshell \"npm run watch\" \"npm run start-server\"",
    "start-server": "node node_modules/http-server/bin/http-server . -a localhost -p 8000",
    "watch": "grunt",
    "postinstall": "parallelshell \"npm run watch\" \"npm run start-server\""

  "license": "MIT",
  "devDependencies": {
    "browserify": "^10.2.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-watch": "^0.6.1",
    "parallelshell": "^1.2.0",
    "http-server": "^0.8.0"
  },
  "dependencies": {
    "socket.io": "^1.3.7",
    "underscore": "^1.8.3"
  }
}
